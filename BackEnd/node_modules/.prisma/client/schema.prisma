datasource db {
  provider = "mysql" // or your preferred database provider

  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model user {
  id        String  @id @default(uuid())
  firstName String?
  lastName  String?
  email     String  @unique
  password  String  @db.VarChar(255)

  @@map("user")
}
